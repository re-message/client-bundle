{
    "name": "relmsg/client-bundle",
    "description": "A Relations Messenger Client Bundle for Symfony",
    "type": "symfony-bundle",
    "license": "Apache-2.0",
    "homepage": "https://dev.relmsg.ru/packages/client-bundle",
    "authors": [
        {
            "name": "Oleg Kozlov",
            "email": "h1karo@relmsg.ru",
            "homepage": "https://rel.ms/h1karo",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "RM\\Bundle\\ClientBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RM\\Bundle\\ClientBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.4",

        "relmsg/client": "^0.4",
        "relmsg/message-bundle": "^0.9",

        "symfony/http-client": "^4.4|^5.0",
        "symfony/event-dispatcher": "^4.4|^5.0",
        "symfony/framework-bundle": "^4.4|^5.0"
    },
    "require-dev": {
        "relmsg/json-web-token": "^1.3",
        "nyholm/psr7": "^1.2",

        "symfony/browser-kit": "^4.4|^5.0",
        "symfony/phpunit-bridge": "^4.4|^5.0",

        "symfony/twig-bundle": "^4.4|^5.0",
        "symfony/twig-bridge": "^4.4|^5.0",
        "symfony/monolog-bundle": "^3.5",
        "symfony/web-profiler-bundle": "^4.4|^5.0",
        "twig/twig": "^2.10|^3.0",

        "vimeo/psalm": "^3.11",
        "psalm/plugin-phpunit": "^0.10",
        "psalm/plugin-symfony": "1.3",
        "roave/security-advisories": "dev-master"
    },
    "conflict": {
        "relmsg/entrypoint-bundle": "<=0.14.2"
    },
    "config": {
        "sort-packages": false
    }
}
